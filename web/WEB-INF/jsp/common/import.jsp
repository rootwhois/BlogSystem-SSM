<%@ page contentType="text/html;charset=UTF-8" language="java" %>
<%@ page pageEncoding="utf-8" %>
<head>
    <title>${webName}</title>

    <!-- meta -->
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">

    <style type="text/css">
        .widget-info {
            text-align: center;
            padding: 48px 16px;
            flex: 1;
        }

        .avatar {
            width: 48px;
            height: 48px;
            width: 80px;
            height: 80px;
            border-radius: 50%;
